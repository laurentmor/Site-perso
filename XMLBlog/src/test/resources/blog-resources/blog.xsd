        suppress XmlHighlighting, XmlHighlighting
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Category" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute type="xs:string" name="ID"/>
                <xs:attribute type="xs:string" name="name"/>
                <xs:attribute type="xs:string" name="description"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="Entry">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Comment">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:string" name="CommentText"/>
                  </xs:sequence>
                  <xs:attribute type="xs:string" name="ID"/>
                  <xs:attribute type="xs:string" name="author"/>
                  <xs:attribute type="xs:string" name="date"/>
                  <xs:attribute type="xs:string" name="entryID"/>
                  <xs:attribute type="xs:string" name="webPage"/>
                </xs:complexType>
              </xs:element>
              <xs:element type="xs:string" name="Resume"/>
              <xs:element type="xs:string" name="Text"/>
            </xs:sequence>
            <xs:attribute type="xs:string" name="ID"/>
            <xs:attribute type="xs:string" name="allowComments"/>
            <xs:attribute type="xs:string" name="categoryID"/>
            <xs:attribute type="xs:date" name="date"/>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
